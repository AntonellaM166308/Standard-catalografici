	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:altLabel xml:lang="it">Monumenti archeologici</skos:altLabel>
	<skos:broader rdf:resource="http://dati.beniculturali.it/vocabularies/settori_disciplinari/def/002.001"/>
