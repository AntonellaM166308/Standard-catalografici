	<skos:narrower rdf:resource="http://dati.beniculturali.it/vocabularies/fondi_fotografici_definizione/def/001.004"/>
	<skos:narrower rdf:resource="http://dati.beniculturali.it/vocabularies/fondi_fotografici_definizione/def/001.005"/>
	<skos:inScheme rdf:resource="http://dati.beniculturali.it/vocabularies/fondi_fotografici_definizione/def/"/>
	<skos:topConceptOf rdf:resource="http://dati.beniculturali.it/vocabularies/fondi_fotografici_definizione/def/"/>
</rdf:Description>

