	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:prefLabel xml:lang="it">serie</skos:prefLabel>
	<skos:definition xml:lang="it">Ciascun raggruppamento di documenti con caratteristiche omogenee, all'interno di un fondo archivistico. Può essere articolata in sottoserie</skos:definition>
	<skos:editorialNote rdf:resource="http://www.archivi.beniculturali.it/index.php/abc-degli-archivi/glossario"/>
	<rdfs:label xml:lang="it">serie</rdfs:label>
